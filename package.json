{
  "name": "@okteto-website/docs",
  "version": "3.1.1",
  "private": true,
  "dependencies": {
    "@docusaurus/core": "^3.9.2",
    "@docusaurus/plugin-client-redirects": "^3.9.2",
    "@docusaurus/plugin-content-pages": "^3.9.2",
    "@docusaurus/preset-classic": "^3.9.2",
    "@docusaurus/theme-mermaid": "^3.9.2",
    "@mdx-js/react": "^3.0.0",
    "@sanity/client": "^3.2.0",
    "classnames": "^2.3.1",
    "docusaurus-gtm-plugin": "^0.0.2",
    "docusaurus-plugin-sass": "^0.2.5",
    "lodash.trimend": "^4.5.1",
    "posthog-docusaurus": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.35.2"
  },
  "engines": {
    "node": ">=22.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "start": "yarn develop",
    "develop": "docusaurus start --host 0.0.0.0 --port 8080 --no-open",
    "build": "docusaurus build --out-dir='build/docs' && mv build/docs/sitemap.xml build/docs/sitemap-docs.xml",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve --host 0.0.0.0 --port 8080",
    "clean": "docusaurus clear",
    "docs": "docusaurus docs:version previous"
  },
  "devDependencies": {
    "baseline-browser-mapping": "^2.9.14"
  }
}
